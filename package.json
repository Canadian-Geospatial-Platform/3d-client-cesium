{
    "name": "Standalone 3D Client",
    "version": "0.1.0",
    "description": "3D client",
    "main": "3dMap.js",
    "scripts": {
        "build-client": "export DOMAIN_NAME=localhost && export $(cat config/env | xargs) && webpack --config ./config/webpack.master.config.js",
        "build-client-release": "export $(cat config/env | xargs) && webpack --config ./config/webpack.master.config.js",
        "build-backend": "export $(cat config/env | xargs) && go build -C ./backend -v -x",
        "full-build": "export DOMAIN_NAME=localhost && export $(cat config/env | xargs) && webpack --config ./config/webpack.master.config.js && go build -C ./backend -v -x",
        "full-build-release": "export $(cat config/env | xargs) && webpack --config ./config/webpack.master.config.js && CGO_CFLAGS='-I/usr/include/gdal' go build -C ./backend -v -x",
        "lint": "eslint 'src/**/*.{ts,tsx,js}' 'src/*.{ts,tsx,js}'",
        "fix": "eslint 'src/**/*.{ts,tsx,js}' 'src/*.{ts,tsx,js}' --fix",
        "dev": "export ROOT_DIR_3D=. && export DOMAIN_NAME=localhost && export $(cat config/env | xargs) && ./backend/run.sh && ./server/run.sh",
        "app": "export $(cat config/env | xargs) && ./backend/run.sh && ./server/run.sh",
        "docker-frontend": "webpack --config ./config/webpack.master.config.js",
        "jenkins-release": "export $(cat config/env | xargs) && npx webpack --config ./config/webpack.master.config.js"
    },
    "keywords": [
        "3d",
        "map",
        "cesium"
    ],
    "author": "Josh Hussey",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.24.3",
